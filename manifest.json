{
  "name": "txtntp",
  "version": "1.0",
  "manifest_version": 2,
  "description": "text new tab page",
  
  "chrome_url_overrides" : {
    "newtab": "txtntp.html"
  }, 
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
  {
    "matches": ["https://github.com/robots.txt*"],
    "js": ["oauth2/oauth2_inject.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "https://github.com/login/oauth/access_token"
  ]

}
